{
  "name": "@suntime-js/core",
  "version": "0.2.0",
  "description": "Evaluate javascript in a sandboxed environment.",
  "type": "module",
  "main": "lib/index.js",
  "private": false,
  "repository": {
    "url": "https://github.com/SunsetFi/static-js"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepublish": "pnpm run clean && pnpm run build",
    "clean": "rimraf lib tsconfig.lib.tsbuildinfo",
    "build": "tsc -p tsconfig.lib.json",
    "check": "tsc -p tsconfig.lib.json --noEmit",
    "test": "vitest",
    "format": "prettier --write ./src",
    "format:staged": "prettier --write",
    "lint": "eslint"
  },
  "keywords": [],
  "author": "SunsetFi",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@babel/types": "^7.28.4",
    "@eslint/js": "^9.23.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "test262-parser": "^2.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "type-fest": "^4.37.0"
  }
}
